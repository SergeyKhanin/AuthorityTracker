.ic {
    -unity-background-scale-mode: stretch-to-fill;
}

.ic-sm {
    width: var(--base-icon-small-size);
    height: var(--base-icon-small-size);
}

.ic-md {
    width: var(--base-icon-middle-size);
    height: var(--base-icon-middle-size);
}

.ic-lg {
    width: var(--base-icon-large-size);
    height: var(--base-icon-large-size);
}

.ic-e-lg {
    width: var(--base-icon-e-large-size);
    height: var(--base-icon-e-large-size);
}

.ic-vr {
    width: var(--base-icon-large-size);
    height: var(--base-icon-small-size);
}

