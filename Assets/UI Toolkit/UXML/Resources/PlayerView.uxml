<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <engine:VisualElement picking-mode="Ignore" style="flex-grow: 1;">
        <engine:VisualElement picking-mode="Ignore" style="flex-grow: 1; flex-direction: row;">
            <engine:Button name="x1-plus-button" style="flex-shrink: 1; flex-grow: 1; background-color: rgba(1, 132, 15, 0.18);" />
            <engine:Button name="x5-plus-button" style="flex-shrink: 1; flex-grow: 1; background-color: rgba(0, 204, 23, 0.17);" />
        </engine:VisualElement>
        <engine:VisualElement picking-mode="Ignore" style="flex-grow: 1; flex-direction: row;">
            <engine:Button name="x1-minus-button" style="flex-shrink: 1; flex-grow: 1; background-color: rgba(161, 7, 7, 0.24);" />
            <engine:Button name="x5-minus-button" style="flex-shrink: 1; flex-grow: 1; background-color: rgba(221, 0, 0, 0.21);" />
        </engine:VisualElement>
    </engine:VisualElement>
    <engine:VisualElement picking-mode="Ignore" class="full-screen">
        <engine:VisualElement picking-mode="Ignore" class="points__image">
            <engine:VisualElement name="points-image" picking-mode="Ignore" />
            <engine:Label name="points-label" picking-mode="Ignore" />
        </engine:VisualElement>
    </engine:VisualElement>
    <engine:VisualElement picking-mode="Ignore" style="flex-grow: 1; position: absolute; height: 100%; width: 100%; align-items: center; justify-content: flex-start;">
        <engine:VisualElement name="counter-image" picking-mode="Ignore" style="flex-grow: 1; display: none; visibility: hidden;" />
        <engine:Label name="counter-label" picking-mode="Ignore" style="font-size: 78px;" />
    </engine:VisualElement>
</engine:UXML>
